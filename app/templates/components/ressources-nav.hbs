<h4 class="text text-sm text-up text-left text-black mb-2 semi-bold">Ressources</h4>
<ul class="unstyled">
  {{#each items as |item|}}
    <li><HandleLink @url="{{item.primary.link.url}}" @linkClass="text text-xs text-left regular text-black-l" @target="{{if item.primary.link.target "_blank"}}" @text="{{as-text item.primary.title}}" /></li>
  {{/each}}
</ul>
