{
  "name": "pix-site",
  "version": "1.9.1",
  "description": "Site vitrine de Pix",
  "author": "Team Pix",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest",
    "release": "npm run release:minor",
    "release:patch": "npm version patch -m \"Release v%s\"",
    "release:minor": "npm version minor -m \"Release v%s\"",
    "release:major": "npm version major -m \"Release v%s\"",
    "preversion": "git checkout v2-dev && git pull --rebase && npm test",
    "postversion": "git push && git checkout v2-master && git pull && git fetch -t && git merge v2-dev --no-edit && git push origin v2-master && git push --tags && git checkout v2-dev"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "nuxt": "^2.0.0",
    "nuxt-i18n": "^6.2.1",
    "nuxt-matomo": "1.2.3",
    "prismic-dom": "^2.1.0",
    "prismic-javascript": "^2.1.1",
    "prismic-vue": "git+https://github.com/prismicio/prismic-vue.git#nuxt",
    "vue-burger-menu": "2.0.2"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/moment": "1.3.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "install": "^0.13.0",
    "jest": "^24.1.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.16.4",
    "sass-loader": "^8.0.0",
    "vue-jest": "^4.0.0-0"
  },
  "engines": {
    "node": "10.15.3"
  }
}
