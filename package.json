{
  "name": "pix-site",
  "version": "3.1.2",
  "private": false,
  "description": "Service en ligne d’évaluation et de certification des compétences numériques pour tous, tout au long de la vie.",
  "repository": {
    "type": "git",
    "url": "git@github.com:1024pix/pix-site.git"
  },
  "license": "AGPL-3.0",
  "author": "Team PIX",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "clean": "rm -rf node_modules .nuxt",
    "build": "./scripts/build.sh && npm run signal-github",
    "build:static": "nuxt build && nuxt export",
    "build:ssr": "nuxt build",
    "start": "./scripts/start.sh",
    "start:static": "nuxt serve",
    "start:ssr": "nuxt start",
    "dev:site": "SITE=pix-site nuxt",
    "build:site": "SITE=pix-site nuxt build && nuxt export",
    "dev:pro": "SITE=pix-pro nuxt",
    "build:pro": "SITE=pix-pro nuxt build && nuxt export",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "SITE=pix-site jest --no-coverage",
    "release": "npm run release:minor",
    "release:patch": "npm version patch -m \"Release v%s\"",
    "release:minor": "npm version minor -m \"Release v%s\"",
    "release:major": "npm version major -m \"Release v%s\"",
    "signal-github": "./scripts/signal_deploy_to_pr.sh"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/vue-fontawesome": "^2.0.0",
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/prismic": "^1.2.4",
    "chart.js": "^2.9.4",
    "lodash": "^4.17.20",
    "nuxt": "^2.13.3",
    "nuxt-fontawesome": "0.4.0",
    "nuxt-i18n": "6.13.1",
    "nuxt-matomo": "1.2.3",
    "nuxt-winston-log": "1.1.0",
    "postcss-cli": "^7.1.1",
    "vue-burger-menu": "2.0.5",
    "vue-chartjs": "3.5.0",
    "vue-js-modal": "1.3.35",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.1",
    "@nuxtjs/moment": "1.6.1",
    "@nuxtjs/style-resources": "^1.0.0",
    "@vue/test-utils": "^1.1.1",
    "autoprefixer": "^10.0.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-nuxt": ">=1.0.0",
    "eslint-plugin-prettier": "3.1.4",
    "install": "^0.13.0",
    "jest": "^26.1.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "sass-loader": "^9.0.2",
    "vue-jest": "3.0.6"
  },
  "engines": {
    "node": "14.15.1"
  }
}
